			"import { decoratedStoriesOf } from 'common/util/storybook';$0"
		]
	},
	"Storybook: Action": {
		"prefix": "import-story-action",
		"description": "Imports 'action' from the Storybook Actions addon",
		"body": [
			"import { action } from '@storybook/addon-actions';$0"
		]
	},
	"Redux: Connect": {
		"prefix": "import-redux-connect",
