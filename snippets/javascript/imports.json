{
    "lodash": {
		"prefix": "import-lo",
		"description": "Imports a lodash function",
		"body": [
			"import ${1:func} from 'lodash/${1:func}';$0"
		]
	},
	"AtlasKit: Button": {
		"prefix": "import-ak-button",
		"description": "Imports AtlasKit Button",
		"body": [
			"import Button from '@atlaskit/button';$0"
		]
	},
	"AtlasKit: Button Group": {
		"prefix": "import-ak-buttongroup",
		"description": "Imports AtlasKit Button Group",
		"body": [
			"import { ButtonGroup } from '@atlaskit/button';$0"
		]
	},
	"AtlasKit: Icon": {
		"prefix": "import-ak-icon",
		"description": "Imports AtlasKit Icon",
		"body": [
			"import ${TM_SELECTED_TEXT/([a-z]*)([A-Z])?/$1${2:+ }${2:/downcase}/g}Icon from '@atlaskit/icon/glyph/${TM_SELECTED_TEXT}';$0"
		]
	},
	"AtlasKit: Spinner": {
		"prefix": "import-ak-spinner",
		"description": "Imports AtlasKit Spinner",
		"body": [
			"import Spinner from '@atlaskit/spinner';$0"
		]
	},
	"AtlasKit: Tooltip": {
		"prefix": "import-ak-tooltip",
		"description": "Imports AtlasKit Tooltip",
		"body": [
			"import Tooltip from '@atlaskit/tooltip';$0"
		]
	},
	"AtlasKit: Colors": {
		"prefix": "import-ak-colors",
		"description": "Imports AtlasKit colors",
		"body": [
			"import { colors } from '@atlaskit/theme';$0"
		]
	},
	"AtlasKit: Grid Unit": {
		"prefix": "import-ak-gridunit",
		"description": "Imports the AtlasKit grid unit",
		"body": [
			"import { gridUnit } from 'common/styles';$0"
		]
	},
	"Storybook: Action": {
		"prefix": "import-story-action",
		"description": "Imports 'action' from the Storybook Actions addon",
		"body": [
			"import { action } from '@storybook/addon-actions';$0"
		]
	},
	"Messages": {
		"prefix": "import-messages",
		"description": "Imports 'messages' from a sibliing messages.js folder",
		"body": [
			"import messages from './messages';$0"
		]
	},
	"Reselect": {
		"prefix": "import-reselect",
		"description": "Imports 'createSelector' and 'Selector' type from reselect",
		"body": [
			"import { createSelector, type Selector } from 'reselect';$0"
		]
	}
}